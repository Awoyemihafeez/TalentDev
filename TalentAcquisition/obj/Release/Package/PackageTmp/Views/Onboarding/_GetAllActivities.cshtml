@model IEnumerable<TalentAcquisition.BusinessLogic.UpdatedDomain.OnboardActivity>

<table class="table table-striped" style="font-size:90%">
     <thead>
        <tr>
            <th>
                Activity Name
            </th>
            <th></th>
        </tr>
    </thead>
<tbody id="activities">
    @foreach (var item in Model)
    {
        <tr>
            <td class="hidden">@Html.DisplayFor(modelItem=>item.ID)</td>
            <td draggable="true" id="activity">
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td><a href="javascript:void(0)" id="addactivity">Add</a></td>
        </tr>
    }
</tbody>

</table>