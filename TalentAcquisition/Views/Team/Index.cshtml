
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<script>
$(document).ready(function(){
  $("#myInput").on("keyup", function() {
    var value = $(this).val().toLowerCase();
    $("#RequisitionTable tr").filter(function() {
      $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
    });
  });
});
</script>
<style>
    input{
        margin-left:0px;
        padding-left:0px;
        margin-bottom:2em;
    }
</style>
<div class="container body-content">
    <div class="col-md-4 adminsidebar">
        <div class="container">
            <br />
            <ul class="nav">
                @Html.Partial("_AdminDashboardSidebar")
            </ul>
        </div>
    </div>
    <div class="col-md-8">
        <h3>Active Requisitions</h3>
        <section class="container body-content">
            <span>@ViewBag.Message</span>
            
            <div class="row">
                <div class="form-group"><label class="">Search </label> <input class="form-control" id="myInput" type="text" placeholder="Search.."></div>
                
                @Html.Action("_GetRequisitionsForTeamMember")
            </div>
        </section>
    </div>

</div>

